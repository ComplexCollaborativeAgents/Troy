sp {topstate*propose*init-agent
	(state <s> -^name
             	^superstate nil)
-->
	(<s> ^operator <op> +)
 	(<op> ^name init-agent)
  }

sp {topstate*apply*init-agent
	(state <s> ^operator.name init-agent)
-->
	(<s> ^topstate <s>
      	^name troy)
 }


