sp {action*elaborate*indexed-action
    (state <s> ^name action
               ^superstate.operator <sop>)
    (<sop> ^indexed-action <ia>)
-->
    (<s> ^indexed-action <ia>)
    }

sp {action*elaborate*segment
    (state <s> ^name action
               ^superstate.operator <sop>)
    (<sop> ^segment <seg>)
-->
    (<s> ^segment <seg>)
    }



sp {action*elaborate*indexed-action*primitive*executed
	(state <s>	^operator <op>
            	^indexed-action <ia>)
 	(<op>	^action-type primitive)
-->
	(<ia>	^executed true)
 }

sp {action*elaborate*dont-match*arguments
	(state <s>	^name action)
-->
	(<s>	^dont-match <dm>)
 	(<dm>	^arg	name
        	^arg	_metadata
         	^arg	executed)
  }

sp {action*elaborate*space
	(state <s>	^name action)
-->
	(<s>	^problemspace <p>)
 	(<p>	^type actionable)
  }
            	