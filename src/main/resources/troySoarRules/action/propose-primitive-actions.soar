   }

sp {action*propose*op_paste
	(state <s> ^name action
               ^world <w>)
    (<w> ^selected <obj>)
-->
	(<s> ^operator <op> +)
    (<op> ^name op_paste
          ^argument <obj>)
    }
               
             	