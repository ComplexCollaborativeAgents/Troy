sp {action*propose*op_list*set
    (state <s> ^name action
               ^world <w>
               -^listed true)
    (<w> ^set <set-obj>)
-->
    (<s> ^operator <op> +)
    (<op> ^name op_list
          ^argument <set-obj>)
    }

sp {action*propose*op_list*obj
    (state <s> ^name action
               ^world <w>
               -^listed true)
    (<w> ^object <obj>)
-->
    (<s> ^operator <op> +)
    (<op> ^name op_list
          ^argument <obj>)
    }

sp {action*propose*op_go_into
	(state <s> ^name action
               ^world <w>)
 	(<w> ^object <obj>)
    (<obj> ^type folder_object)
-->
	(<s> ^operator <op> +)
 	(<op> ^name op_go_into
          ^argument <obj>)
  }

sp {action*propose*op_create_folder
	(state <s> ^name action
            	^world <w>)
 	-{(<w> ^object <obj>)
      (<obj> ^type folder_object
             ^name <name>)}
-->
	(<s> ^operator <op> +)
 	(<op> ^name op_create_folder
          ^argument1 <name>
          ^argument2 folder_object)
  }

sp {action*propose*op_copy
	(state <s> ^name action
            	^world <w>)
    (<w> ^object <obj>
         -^selected <obj>)
-->
	(<s> ^operator <op> +)
 	(<op> ^name op_copy
          ^argument <obj>)
  }

sp {action*propose*op_cut
   (state <s> ^name action
              ^world <w>)
   (<w> ^object <obj>
        -^selected <obj>)
