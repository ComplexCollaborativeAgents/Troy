sp {action*apply*op_cut
	(state <s> ^operator <op>
               ^io.output-link <ol>)
    (<op> ^name op_cut
          ^argument <obj>)
    (<obj> ^type <type>)
-->
    (<ol> ^command <c>)
    (<c> ^name cut
         ^<type> <obj>)
    }


## model
sp {action*apply*op_cut*model
	(state <s>	^operator <op>
             	-^io
               	^world <w>)
 	(<op>	^name op_cut
        	^argument <obj>)
-->
	(<w>	^selected <obj>)
 }