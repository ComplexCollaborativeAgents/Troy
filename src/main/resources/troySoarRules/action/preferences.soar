    (state <s> ^name action
               ^indexed-action <ia>
               ^operator <op> +
          		-^dont-match.arg <argument>)
    (<ia> ^name <op_name>
          ^<argument> <ia-arg>)
    (<op> ^name <op_name>
           ^<argument> {<> <ia-arg>})
-->
    (<s> ^operator <op> -)
    }