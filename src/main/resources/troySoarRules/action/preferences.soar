sp {action*reject*operator*different-name
    (state <s> ^name action
               ^indexed-action <ia>
               ^operator <op> +)
    (<ia> ^op_name <ia_name>)
    (<op> ^name {<op_name> <> <ia_name>}
          -^type maintenance)
-->
    (<s> ^operator <op> -)
    }

sp {action*reject*operator*different-argument
    (state <s> ^name action
               ^indexed-action <ia>
               ^operator <op> +)
    (<ia> ^{<argument> <> op_name} <ia-arg>)
    (<op> ^<argument> {<op-arg> <> <ia-arg>}
          -^type maintenance)
-->
    (<s> ^operator <op> -)
    }