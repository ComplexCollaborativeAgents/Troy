sp {index-noun-phrase*propose*find-object
    (state <s> ^name index-noun-phrase
               ^description.noun <noun>
               ^world.objects <objs>
               -^found-objects.description <noun>)   
-->
    (<s> ^operator <op> +)
    (<op> ^name find-object
          ^object-set <objs>
          ^description <noun>)
    }
               

sp {index-noun-phrase*propose*resolve-grounding
    (state <s> ^name index-noun-phrase
               ^grounding <g>
               ^resolution <res>
               ^found-objects.objects <objs>)
    (<g> -^<any>)
-->
    (<s> ^operator <op> +)
    (<op> ^name resolve-grounding
          ^grounding <g>
          ^resolution <res>
          ^found-objects.objects <objs>)
    }

sp {index-noun-phrase*propose*complete-index-noun-phrase
    (state <s> ^name index-noun-phrase)
-->
    (<s> ^operator <op> + <)
    (<op> ^name complete-index-noun-phrase)
    }
               