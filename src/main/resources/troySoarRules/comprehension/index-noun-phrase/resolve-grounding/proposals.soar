sp {resolve-grounding*propose*resolve-collection
    (state <s> ^name resolve-grounding
               ^grounding <g>
               ^objects <objs>
               ^resolution <res>)
    (<res> ^arity multiple
           ^preference all)
-->
    (<s> ^operator <op> +)
    (<op> ^name resolve-collection
          ^grounding <g>
          ^objects <objs>)
    }

sp {resolve-grounding*propose*resolve-single*equal
    (state <s> ^name resolve-grounding
               ^objects <objs>
               ^resolution <res>
               ^grounding <g>)
    (<objs> ^object <obj>)
    (<res> ^arity single
           ^preference equal)
-->
    (<s> ^operator <op> + =)
    (<op> ^name resolve-single-equal
          ^grounding <g>
          ^object <obj>)
    }