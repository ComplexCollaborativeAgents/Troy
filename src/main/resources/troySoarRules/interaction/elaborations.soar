sp {interaction*elaborate*message
    (state <s> ^name interaction
               ^superstate.operator <sop>)
    (<sop> ^name interaction
           ^message <m>
           ^interaction <inter>)
-->
    (<s> ^message <m>
         ^interaction <inter>)
    }

sp {interaction*elaborate*processing-status
	(state <s>	^name interaction
            	^superstate.operator <sop>)
 	(<sop>	^name interaction
         	^processing-status <status>
          	^interaction <inter>)
-->
	(<s>	^processing-status <status>
      		^interaction <inter>)
 }
