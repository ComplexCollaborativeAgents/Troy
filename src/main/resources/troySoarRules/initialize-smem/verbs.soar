smem --add {

	### list objects
    (<a1> ^linguistic <al1> ^procedural <ap1> ^type action)
    (<al1> ^verb list ^entity <arg1>)
    (<ap1> ^op_name op_list ^argument <arg1>)
    
    ### go into a folder
	(<a2> ^linguistic <al2> ^procedural <ap2> ^type action)
 	(<al2> ^verb go ^entity <arg2>)
  	(<ap2> ^op_name op_go_into ^argument <arg2>)
   
   ### create a folder
	(<a3> ^linguistic <al3> ^procedural <ap3> ^type action)
 	(<al3> ^verb create ^entity <arg31>)
  	(<ap3> ^op_name op_create_folder ^argument1 <arg31>)
    (<arg31> ^default folder_object)
    
   ### copy a file
    (<a4> ^linguistic <al4> ^procedural <ap4> ^type action)
    (<al4> ^verb copy ^entity <arg41>)
    (<ap4> ^op_name op_copy ^argument <arg41>)
    
    ### paste a file
    (<a5> ^linguistic <al5> ^procedural <ap5> ^type action)
    (<al5> ^verb paste ^entity <arg51>)
    (<ap5> ^op_name op_paste ^argument <arg51>)
    
    ### cut a file
    (<a6> ^linguistic <al6> ^procedural <ap6> ^type action)
    (<al6> ^verb cut ^entity <arg61>)
    (<ap6> ^op_name op_cut ^argument <arg61>)
}