

sp {propose*wait
   (state <s> ^attribute state
              ^choices none
             -^operator.name wait)
-->
   (<s> ^operator <o> +)
   (<o> ^name wait)
   }

sp {propose*copy*otto-message
	(state <s> ^superstate nil
            	^io.input-link.interaction <itr-l>
             	-^message)
 	(<itr-l> ^<any> <m>)
-->
	(<s> ^operator <op> +)
 	(<op> ^name copy-interaction
          ^message <m>)
  	
  }

sp {apply*copy*otto-message
	(state <s> ^operator <op>)
 	(<op> ^name copy-interaction
          ^message <m>)
-->
    (<s> ^message <m>)
    }

sp {propose*action-command*response
	(state <s> ^message <m>
            	-^responded true)
 	(<m> ^type action-command)
-->
	(<s> ^operator <op> +)
 	(<op> ^name respond-unknown-task
          ^action-command <m>) 
 }

sp {apply*action-command*message
	(state <s> ^operator <op>
            	^io.output-link <ol>)
 	(<op> ^name respond-unknown-task
          ^action-command <m>)
-->
	(<ol> ^message <message>)
 	(<message> ^dialog-act inform
       	 ^content unknown-task)
 	(<s> ^responded true)
 }
          
          
          
            
            